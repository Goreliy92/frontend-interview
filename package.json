{
  "name": "frontend-interview-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for frontend mentorship with JS, TS, and DOM API exercises",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --reporter=verbose",
    "test:filter": "vitest --reporter=verbose"
  },
  "keywords": [
    "frontend",
    "interview",
    "mentorship",
    "javascript",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitest/ui": "^4.0.8",
    "jsdom": "^25.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^4.0.8"
  }
}
